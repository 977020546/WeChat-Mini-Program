.swiper{
    width: 750rpx;
    height: 340rpx;
}
.image{
    width: 100%;
}
.index_cate{
    display: flex;
}
.navigator_cate{
    flex: 1;
    padding: 20rpx;
}
.cate_image{
    width: 100%;
}
.title_image{
    width: 100%;
}
.floor_title{
    padding: 10rpx 0;
}
.floor_list{
    /* 清楚浮动 */
    overflow: hidden;
}
.floor_navigator{
    float: left;
    width: 33.33%;
}
/* 后四个超链接 */
.floor_navigator:nth-last-child(-n+4){
/* 原图的宽高 232 * 386 */
    /* 232 / 386 = 33.33vw / height */
    /* 第一张图片的高度 height: 33.33vw * 386 / 232; */
    /* height: 33.33vw * 386 / 232 / 2; */
    height:27.72711807vw;
    border-left: 10rpx solid #fff;
}
.floor_navigator:nth-child(1){
    height: 55vw;
    padding-left: 10rpx;
}
.floor_navigator:nth-child(2),
.floor_navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.floor_navigator:nth-child(3),
.floor_navigator:nth-child(5){
  padding-right:10rpx ;
}
.list_image{
    width: 100%;
    height: 100%;
}